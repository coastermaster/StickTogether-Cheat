(function(){function e(){const t=document.querySelectorAll('.active');if(t.length===0)return void clearInterval(n);t.forEach(o=>{setTimeout(()=>{const{left:e,width:r,top:a,height:i}=o.getBoundingClientRect(),c=e+r/2,d=a+i/2,s=new MouseEvent('mousedown',{view:window,bubbles:!0,cancelable:!0,clientX:c,clientY:d});document.elementFromPoint(c,d).dispatchEvent(s)},1)})}const n=setInterval(e,1)})();
