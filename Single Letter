javascript:(function(){const elements=document.querySelectorAll('.letters-root.scale-1 .tx.active');elements.forEach((element,index)=>{setTimeout(()=>{const{left,width,top,height}=element.getBoundingClientRect();const x=left+width/2,y=top+height/2;const mouseEvent=new MouseEvent('mousedown',{view:window,bubbles:true,cancelable:true,clientX:x,clientY:y});document.elementFromPoint(x,y).dispatchEvent(mouseEvent);},1);});})();
